# http://editorconfig.org

root = true

[*]
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true
end_of_line = lf
charset = utf-8

[*.html]
indent_size = 2

[*.{js,vue,json}]
indent_size = 2
insert_final_newline = ignore

[**.min.js]
indent_style = ignore
insert_final_newline = ignore
